{
  "name": "@poviolabs/ecs-deploy-cli",
  "version": "0.10.8",
  "packageManager": "yarn@3.1.1",
  "scripts": {
    "lint": "exlint --fix",
    "test:ts-node": "STAGE=test ts-node ./src/index.ts",
    "test": "STAGE=test ./dist/index.js",
    "build": "yarn version patch && yarn ncc build ./src/index.ts -m --no-source-map-register && chmod +x ./dist/index.js",
    "clean": "rm ./dist/*"
  },
  "keywords": [
    "povio",
    "spa",
    "deploy",
    "aws",
    "s3",
    "cloudfront"
  ],
  "homepage": "https://github.com/poviolabs/ecs-deploy-cli",
  "bugs": "https://github.com/poviolabs/ecs-deploy-cli/issues",
  "repository": {
    "url": "https://github.com/poviolabs/ecs-deploy-cli",
    "type": "git"
  },
  "contributors": [
    "Marko Zabreznik <marko.zabreznik@poviolabs.com>"
  ],
  "bin": "./dist/index.js",
  "devDependencies": {
    "@aws-sdk/client-ecr": "^3.47.0",
    "@aws-sdk/client-ecs": "^3.47.0",
    "@aws-sdk/node-config-provider": "^3.47.0",
    "@swc/core": "^1.2.123",
    "@types/diff": "^5.0.2",
    "@types/eslint": "^8.2.1",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/node": "^16.11.17",
    "@types/prettier": "^2.4.2",
    "@types/prompt-sync": "^4.1.1",
    "@types/semver": "^7.3.9",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "@vercel/ncc": "^0.33.1",
    "chalk": "^4.1.2",
    "diff": "^5.0.0",
    "docker-cli-js": "2.9.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import-helpers": "^1.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "prompt-sync": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "semver": "^7.3.5",
    "simple-git": "^2.48.0",
    "ts-node": "^10.4.0",
    "typescript": "4.5.4",
    "yargs": "^17.3.1"
  },
  "dependencies": {
    "@aws-sdk/credential-provider-env": "^3.47.0",
    "@aws-sdk/credential-provider-ini": "^3.47.0"
  }
}
